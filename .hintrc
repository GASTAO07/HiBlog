{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/parsing": [
      "default",
      {
        "duplicate-id-aria": "off"
      }
    ],
    "typescript-config/strict": "off",
    "axe/forms": [
      "default",
      {
        "select-name": "off",
        "label": "off"
      }
    ],
    "axe/structure": [
      "default",
      {
        "list": "off",
        "listitem": "off"
      }
    ],
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ],
    "meta-viewport": "off",
    "no-inline-styles": "off",
    "axe/text-alternatives": "off",
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off"
      }
    ]
  }
}